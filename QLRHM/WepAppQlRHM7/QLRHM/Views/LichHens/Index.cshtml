@model IEnumerable<QLRHM.Models.LichHen>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Home.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaLichHen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LyDo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayHen)
