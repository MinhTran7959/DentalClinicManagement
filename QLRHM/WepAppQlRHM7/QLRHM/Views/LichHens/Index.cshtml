            <td>
                @Html.DisplayFor(modelItem => item.NgaySua)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdndkhNavigation.Idndkh)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idlh">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idlh">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idlh">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
